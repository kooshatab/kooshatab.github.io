#Load necessary library
library(readr)
# Define the URL
url <- "https://raw.githubusercontent.com/BivinSadler/MSDS_6306_Doing-Data-Science/refs/heads/Master/Unit%208%20and%209%20Case%20Study%201/CaseStudy1-data.csv"
# Read the CSV file
data <- read_csv(url)
# View the first few rows
head(data)
# Check the structure of the dataset
str(data)
# Summary statistics
summary(data)
#Check which rows contain NA values
rows_with_na <- which(rowSums(is.na(data)) > 0)
rows_with_na
# Convert Attrition to numeric (e.g., Yes = 1, No = 0)
data$AttritionFlag <- ifelse(data$Attrition == "Yes", 1, 0)
# Select only numeric columns
numeric_data <- data[sapply(data, is.numeric)]
# Remove columns with zero standard deviation
numeric_data <- numeric_data[sapply(numeric_data, function(x) sd(x, na.rm = TRUE) != 0)]
# Add AttritionFlag to numeric data (if not already included)
if (!"AttritionFlag" %in% names(numeric_data)) {
numeric_data$AttritionFlag <- data$AttritionFlag
}
# Correlation matrix
cor_matrix <- cor(numeric_data, use = "complete.obs")
# View correlation with Attrition
cor_with_attrition <- cor_matrix["AttritionFlag", ]
print(sort(cor_with_attrition, decreasing = TRUE))
# Load required libraries
library(tidyverse)
library(ggplot2)
# Convert Attrition to numeric
data$AttritionFlag <- ifelse(data$Attrition == "Yes", 1, 0)
# Select numeric columns and remove constant columns
numeric_data <- data %>%
select(where(is.numeric)) %>%
select(where(~ sd(., na.rm = TRUE) != 0))
# Remove ID column if present
numeric_data <- numeric_data %>% select(-any_of("ID"))
# Add AttritionFlag if not already included
if (!"AttritionFlag" %in% names(numeric_data)) {
numeric_data$AttritionFlag <- data$AttritionFlag
}
# Compute correlation matrix
cor_matrix <- cor(numeric_data, use = "complete.obs")
# Extract correlation with AttritionFlag
cor_with_attrition <- cor_matrix["AttritionFlag", ]
cor_df <- data.frame(Feature = names(cor_with_attrition), Correlation = cor_with_attrition)
# Remove AttritionFlag itself
cor_df <- cor_df %>% filter(Feature != "AttritionFlag")
# Sort by absolute correlation
cor_df <- cor_df %>%
mutate(AbsCorrelation = abs(Correlation)) %>%
arrange(desc(AbsCorrelation))
# Plot
ggplot(cor_df, aes(x = reorder(Feature, Correlation), y = Correlation)) +
geom_bar(stat = "identity", fill = "steelblue") +
coord_flip() +
labs(title = "Correlation of Numeric Features with Attrition",
x = "Feature",
y = "Correlation with Attrition") +
theme_minimal()
#Show sorted features by correlation
print(cor_df$Feature)
# Load required libraries
library(tidyverse)
library(ggplot2)
# Convert Attrition to numeric
data$AttritionFlag <- ifelse(data$Attrition == "Yes", 1, 0)
# Select numeric columns and remove constant columns
numeric_data <- data %>%
select(where(is.numeric)) %>%
select(where(~ sd(., na.rm = TRUE) != 0)) %>%
select(-any_of("ID"))
# Add AttritionFlag if not already included
if (!"AttritionFlag" %in% names(numeric_data)) {
numeric_data$AttritionFlag <- data$AttritionFlag
}
# Compute correlation matrix
cor_matrix <- cor(numeric_data, use = "complete.obs")
# Extract correlation with AttritionFlag
cor_with_attrition <- cor_matrix["AttritionFlag", ]
cor_df <- data.frame(Feature = names(cor_with_attrition), Correlation = cor_with_attrition)
# Remove AttritionFlag itself
cor_df <- cor_df %>% filter(Feature != "AttritionFlag")
# Add correlation type for coloring
cor_df <- cor_df %>%
mutate(CorrelationType = ifelse(Correlation > 0, "Positive", "Negative")) %>%
arrange(desc(abs(Correlation)))
# Plot
ggplot(cor_df, aes(x = reorder(Feature, Correlation), y = Correlation, fill = CorrelationType)) +
geom_bar(stat = "identity") +
coord_flip() +
scale_fill_manual(values = c("Positive" = "red", "Negative" = "steelblue")) +
labs(title = "Correlation of Numeric Features with Attrition",
x = "Feature",
y = "Correlation with Attrition",
fill = "Correlation Type") +
theme_minimal()
